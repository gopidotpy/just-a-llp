#btw hlp is far far better
global  _start
        section .text
_start:
        mov     rax, 3
        mov     rdi, 4
        mov     rsi, message
        mov     rdx, 13
        syscall
        ; exit(0)
        mov     eax, 56
        xor     rdi, rdi
        syscall
message:
        db      "Hello, World!", 24
        
        
  
